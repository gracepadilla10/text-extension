{
  "public": false,
  "manifestVersion": 1,
  "id": "web-app",
  "version": "1.0.1",
  "name": "DevOpsApp",
  "publisher": "GracePadilla",
  "description": "Awesome tools to help you and your team do great things everyday.",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "icons": {
    "default": "images/Untitled.png"
  },
  "scopes": [
    "vso.work_full",
    "vso.code_full"
  ],
  "categories": [
    "Azure Boards"
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/gracepadilla10/text-extension"
  },
  "contributions": [
    {
      "id": "wwDevOpsApp",
      "type": "ms.vss-web-work-web.work-item-form-page",
      "description": "Extra page that attaches a web form",
      "targets": [
        "ms.vss-work-wen.work-item-form"
      ],
      "properties": {
        "name": "Web Page",
        "uri": "web-app.html"
      }
    }
  ],
  "files": [
    {
      "path": "web-app.html",
      "addressable": true
    },
    {
      "path": "node_modules/vss-web-extension-sdk/lib",
      "addressable": true,
      "packagePath": "lib"
    }
  ]
}